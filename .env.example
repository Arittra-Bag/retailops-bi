# RetailOps BI - Environment Configuration Example
# Copy this file to .env and update with your actual credentials

# Snowflake Configuration (Required for SQL Analytics)
SNOWFLAKE_ACCOUNT=your_account_id.region.cloud_platform
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=RETAIL_BI
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_SCHEMA=RAW

# AI Services (Optional - for enhanced features)
GEMINI_API_KEY=your_gemini_api_key
LANGSMITH_API_KEY=your_langsmith_api_key

# Application Settings
DEBUG=False
LOG_LEVEL=INFO 